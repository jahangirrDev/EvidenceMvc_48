@using EvidenceMvc_48
@inherits System.Web.Mvc.WebViewPage<List<Course>>



    <table>
        <tr>
            <td>Id</td>
            <td>course Name</td>
            <td>Action</td>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Course_Name</td>
                <td>
                    <a href="Home/Edit/@item.Id">Edit</a> &nbsp;
                    <a href="Home/Delete/@item.Id">Delete</a>
                </td>
            </tr>
        }

    </table>



